<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.wj.dao.AdminRolePermissionDao">
    <resultMap type="com.example.wj.pojo.AdminRolePermission" id="AdminRolePermissionmap">
        <id column="id" property="id"/>
        <result column="rid" property="rid"/>
        <result column="pid" property="pid"/>
    </resultMap>
    <select id="findAllByRid" parameterType="java.util.List" resultType="Integer">
        select pid from admin_role_permission
         where rid in
        <foreach collection="list" item="id" open="(" close=")" separator="," >
             #{id}
        </foreach>

    </select>


</mapper>
